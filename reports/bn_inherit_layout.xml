<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="bold_external_layout_inherit" inherit_id="web.external_layout_bold">
        <xpath expr="//div[@name='company_address']" position="replace"></xpath>
        <!-- replace footer-->
        <xpath expr="//div[last()]" position="replace"/>
    </template>
    <template id="striped_external_layout_inherit" inherit_id="web.external_layout_striped">
        <xpath expr="//div[@name='company_address']" position="replace"></xpath>
        <!-- replace footer-->
        <xpath expr="//div[last()]" position="replace"/>
    </template>
</odoo>